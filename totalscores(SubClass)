public class totalScores extends playerScores
{
   float avg1;
   float avg2;
   float avg3;
   
   
   public totalScores()
   {
	   super();
			this.Score1 += Score2; //Totals the scores for team one
			Score1 = keyboard.nextInt(); 
			Score2 = keyboard.nextInt();
			System.out.println();
			System.out.println("The total score for team " + totalScore1 + " was: ");
			
			this.Score3 += Score4; //Totals the scores for team two
			Score3 = keyboard.nextInt(); 
			Score4 = keyboard.nextInt();
			System.out.println();
			System.out.println("The total score for team " + totalScore2 + " was: ");
			
			this.Score5 += Score6; //Totals the scores for team three
			Score5 = keyboard.nextInt(); 
			Score6 = keyboard.nextInt();
			System.out.println();
			System.out.println("The total score for team " + totalScore1 + " was: ");
	   }   
	   
   public double avg()
   {
	   		avg1 = (float) totalScore1/teamSize; //Average score for team one
			System.out.println();
			System.out.println("The average score for team " + totalScore1 + " was: " + avg1);
			return avg1;
		
   }		
   {			avg2 = (float) totalScore2/teamSize; //Average score for team two
			System.out.println();
			System.out.println("The average score for team " + totalScore2 + " was: " + avg1);
				
				
			avg3 = (float) totalScore3/teamSize; //Average score for team three
			System.out.println();
			System.out.println("The average score for team " + totalScore3 + " was: " + avg1);
					
	   }   
	   
